{
    "name": "vscode-opennewinstance",
    "displayName": "vscode-openNewInstance",
    "description": "Open a new instance of VS Code from the Explorer",
    "version": "0.0.1",
    "publisher": "chrisdias",
    "engines": {
        "vscode": "^1.0.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [{
            "command": "extension.openNewInstance",
            "title": "Open New Instance"
        }],
        "menus": {
            "explorer/context": [
                {
                    "command": "extension.openNewInstance"
                }
            ]
        }
    },
    "scripts": {
        "vscode:prepublish": "node ./node_modules/vscode/bin/compile",
        "compile": "node ./node_modules/vscode/bin/compile -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "typescript": "^1.8.5",
        "vscode": "^0.11.0"
    }
}